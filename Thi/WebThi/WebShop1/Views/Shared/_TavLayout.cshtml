

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Electronix Store</title>
    <meta http-equiv="Content-Type" content="text/html; charset=windows-1252" />
    <link rel="stylesheet" type="text/css" href="~/Content/style1.css" />
    <!--[if IE 6]>-->
    <!-- <link rel="stylesheet" type="text/css" href="~/Content/iecss1.css" />
     <!--[endif]-->
    <script type="text/javascript" src="~/Scripts/boxOver.js"></script>
</head>
<body>
    <div id="main_container">
        @RenderPage("~/Views/Shared/_TopBar.cshtml");
        @RenderPage("~/Views/Shared/_Header.cshtml");

        <div id="main_content">
            @RenderPage("~/Views/Shared/_MenuTad.cshtml");

            <!-- end of menu tab -->
            <div class="crumb_navigation"> Navigation: <span class="current">Home</span> </div>
            @RenderPage("~/Views/Shared/_MenuLeft.cshtml");

            <!-- end of left content -->
            <div class="center_content">
                @RenderBody();
            </div>
            <!-- end of center content -->
            @RenderPage("~/Views/Shared/_MenuRight.cshtml");

            <!-- end of right content -->
        </div>
        <!-- end of main content -->

        @RenderPage("~/Views/Shared/_Footer.cshtml");


    </div>
    <!-- end of main_container -->

</body>
</html>


